<https://w3id.org/kgviz/> a owl:Ontology ;
    rdfs:label "kgviz" ;
    dcterms:license "https://creativecommons.org/publicdomain/zero/1.0/" ;
    dcterms:title "Knowledge Graph Visualization Configuration" ;
    rdfs:seeAlso "https://github.com/cmungall/obographviz" ;
    skos:definition """A data model for describing configurations / stylesheets for visualzing graphs, and in particular Knowledge Graphs or Ontologies. These graphs are characterized by having meaningful edge labels, node categories, IDs or URIs on each element, as well as additional rich metadata on the nodes or edges.
An example of a use of this is https://github.com/cmungall/obographviz""" ;
    linkml:generation_date "2021-05-14 18:22" ;
    linkml:metamodel_version "1.7.0" ;
    linkml:source_file "kgviz.yaml" ;
    linkml:source_file_date "Fri May 14 18:21:54 2021" ;
    linkml:source_file_size 4595 .

<https://w3id.org/kgviz/StyleSheet> a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "StyleSheet" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/kgviz/Node> ;
            owl:onProperty <https://w3id.org/kgviz/displayAnnotations> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://UNKNOWN.org/StyleType> ;
            owl:onProperty <https://w3id.org/kgviz/graphvizstyles> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://UNKNOWN.org/ReasoningType> ;
            owl:onProperty <https://w3id.org/kgviz/reasoning> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <https://w3id.org/kgviz/graphvizColor> ;
            owl:onProperty <https://w3id.org/kgviz/graphvizfillcolor> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/kgviz/ConditionalProperty> ;
            owl:onProperty <https://w3id.org/kgviz/conditionalProperties> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/kgviz/PrefixConfiguration> ;
            owl:onProperty <https://w3id.org/kgviz/prefixProperties> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/kgviz/Node> ;
            owl:onProperty <https://w3id.org/kgviz/cliqueRelations> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <https://w3id.org/kgviz/Condition> ;
            owl:onProperty <https://w3id.org/kgviz/nodeFilter> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/kgviz/RelationConfiguration> ;
            owl:onProperty <https://w3id.org/kgviz/relationProperties> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/kgviz/Node> ;
            owl:onProperty <https://w3id.org/kgviz/highlightIds> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Boolean ;
            owl:onProperty <https://w3id.org/kgviz/excludeSingletons> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <http://UNKNOWN.org/StyleType> ;
            owl:onProperty <https://w3id.org/kgviz/graphvizstyle> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty <https://w3id.org/kgviz/graphvizlabelFrom> ] ;
    skos:definition "A configuration for both global graph-level properties and element-level properties indicating how a KG should be rendered graphically" .

