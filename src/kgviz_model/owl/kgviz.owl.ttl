<https://w3id.org/kgviz/Condition> a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Condition" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty <https://w3id.org/kgviz/subset> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty <https://w3id.org/kgviz/type> ] ;
    skos:definition "a constraint or condition to be applied, e.g. type=Class" .

<https://w3id.org/kgviz/graphvizcolor> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "color" ;
    rdfs:range <https://w3id.org/kgviz/graphvizColor> ;
    rdfs:subPropertyOf <https://w3id.org/kgviz/graphvizcluster_property>,
        <https://w3id.org/kgviz/graphvizedge_property>,
        <https://w3id.org/kgviz/graphviznode_property> ;
    skos:definition "Basic drawing color for graphics, not text. For the latter, use the fontcolor attribute." .

<https://w3id.org/kgviz/graphvizfontcolor> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "fontcolor" ;
    rdfs:range <https://w3id.org/kgviz/graphvizColor> .

<https://w3id.org/kgviz/graphvizlabel> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "label" ;
    rdfs:range linkml:String .

<https://w3id.org/kgviz/graphvizpenwidth> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "penwidth" ;
    rdfs:range linkml:Integer .

<https://w3id.org/kgviz/graphvizGraphvizAspect> a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "graphviz aspect" ;
    rdfs:seeAlso "https://graphviz.org/doc/info/attrs.html#h:uses" ;
    rdfs:subClassOf linkml:mixin .

<https://w3id.org/kgviz/graphvizgraphviz_slot> a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "graphviz slot" ;
    rdfs:range linkml:String ;
    rdfs:seeAlso "https://graphviz.org/doc/info/attrs.html#h:uses" .

linkml:Integer a owl:Class,
        linkml:TypeDefinition ;
    rdfs:label "integer" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:integer ;
            owl:onProperty linkml:topValue ;
            owl:qualifiedCardinality 1 ] ;
    skos:definition "An integer" .

<https://w3id.org/kgviz/Node> a owl:Class,
        linkml:TypeDefinition ;
    rdfs:label "Node" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty linkml:topValue ;
            owl:qualifiedCardinality 1 ] ;
    skos:definition "e.g. UBERON:0002101" .

<https://w3id.org/kgviz/graphvizColor> a owl:Class,
        linkml:TypeDefinition ;
    rdfs:label "Color" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty linkml:topValue ;
            owl:qualifiedCardinality 1 ] ;
    skos:definition "e.g. red, green" .

linkml:ClassDefinition a owl:Class ;
    rdfs:label "class_definition" ;
    skos:definition "the definition of a class or interface" .

linkml:TypeDefinition a owl:Class ;
    rdfs:label "type_definition" ;
    skos:definition "A data type definition." .

linkml:topValue a owl:DatatypeProperty ;
    rdfs:label "value" .

linkml:String a owl:Class,
        linkml:TypeDefinition ;
    rdfs:label "string" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty linkml:topValue ;
            owl:qualifiedCardinality 1 ] ;
    skos:definition "A character string" .

linkml:SlotDefinition a owl:Class ;
    rdfs:label "slot_definition" ;
    skos:definition "the definition of a property or a slot" .



